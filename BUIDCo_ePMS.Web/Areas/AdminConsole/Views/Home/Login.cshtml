<link href="~/AdminConsole/css/login.css" rel="stylesheet">

<div class="section">
   <form asp-controller="Home" asp-action="Login" method="post">
                    <div class="login-body">
            <h3> <img src="~/adminconsole/images/loginbg.png">   <i>Admin Console Login</i></h3>
                        <div class="form-group row">
                            <label class="col-12 col-md-4 col-xl-4 control-label" for="demo-email-input">User Name</label>
                            <div class="col-12 col-md-8 col-xl-8">
                                <span class="colon">:</span>
                                <input type="text" id="txtUName" class="form-control" maxlength="45" asp-for="UserName" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-md-4 col-xl-4 control-label" for="demo-email-input">Password</label>
                            <div class="col-12 col-md-8 col-xl-8">
                                <span class="colon">:</span>
                                <input type="password" maxlength="100" id="txtPwd" class="form-control" asp-for="UserPasswaord" s />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-md-4 col-xl-4 control-label" for="demo-email-input"> <span class="text-danger"></span></label>
                            <div class="col-12 col-md-8 col-xl-8" style="text-align: left;">
                                <span class="colon"></span>
                                <input type="submit" id="btnSubmit" value="Login" class="btn btn-primary" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-md-4 col-xl-4 control-label danger"></label>@ViewBag.DisplayMessage
                        </div>
                    </div>
                </form>
</div>
